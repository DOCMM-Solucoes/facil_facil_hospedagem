<p>
  <%= render @photo %>
</p>

<div class="field">
  <% @photo.images.each do |image| %>
    <%= image_tag image, class: 'image-size' %>
  <% end %>

</div>

<div>
  <%= link_to "Voltar para estrututras", photos_path %>
  <% if user_signed_in? && current_user %>
    <%= link_to "Editar as estrutura", edit_photo_path(@photo) %> |
    <%= button_to "Deletar estrutura", @photo, method: :delete,
                form: { data: { turbo_confirm: 'VocÃª tem certeza' } } %>
  <% end %>
</div>
