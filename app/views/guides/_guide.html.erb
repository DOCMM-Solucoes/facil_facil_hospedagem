<div id="<%= dom_id guide %>">
  <p>
    <strong><%= t'app.guide.guide'%></strong>
    <%= guide.name %>
  </p>

  <p>
    <strong><%= t'app.guide.phone'%>:</strong>
    <%= guide.phone %>
  </p>

  <p>
    <strong><%= t'app.guide.cpf'%></strong>
    <%= guide.cpf %>
  </p>

  <p>
    <strong><%= t'app.guide.company'%></strong>
    <%= guide.company %>
  </p>

  <p>
    <strong><%= t'app.guide.cnpj'%></strong>
    <%= guide.cnpj %>
  </p>

  <p>
    <strong><%= t('app.hotelier.address_street') %>:</strong>
    <%= guide.address.street %>
  </p>

  <p>
    <strong><%= t('app.hotelier.address_postal_code') %>:</strong>
    <%= guide.address.postal_code %>
  </p>

  <p>
    <strong><%= t('app.hotelier.address_city') %>:</strong>
    <%= guide.address.city %>
  </p>

  <p>
    <strong><%= t('app.hotelier.address_state') %>:</strong>
    <%= guide.address.state %>
  </p>

</div>
